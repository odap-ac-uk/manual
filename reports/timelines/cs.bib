@article{roach2018lakelouise2018,
 abstract = {Roach, Robert C., Peter H. Hackett, Oswald Oelz, Peter Ba\textasciidieresis rtsch, Andrew M. Luks, Martin J. MacInnis, J. Kenneth Baillie, and The Lake Louise AMS Score Consensus Committee. The 2018 Lake Louise Acute Mountain Sickness Score. High Alt Med Biol. 19:4\textendash 6, 2018.\textemdash{} The Lake Louise Acute Mountain Sickness (AMS) scoring system has been a useful research tool since first published in 1991. Recent studies have shown that disturbed sleep at altitude, one of the five symptoms scored for AMS, is more likely due to altitude hypoxia per se, and is not closely related to AMS. To address this issue, and also to evaluate the Lake Louise AMS score in light of decades of experience, experts in high altitude research undertook to revise the score. We here present an international consensus statement resulting from online discussions and meetings at the International Society of Mountain Medicine World Congress in Bolzano, Italy, in May 2014 and at the International Hypoxia Symposium in Lake Louise, Canada, in February 2015. The consensus group has revised the score to eliminate disturbed sleep as a questionnaire item, and has updated instructions for use of the score.},
 author = {Roach, Robert C. and Hackett, Peter H. and Oelz, Oswald and B{\"a}rtsch, Peter and Luks, Andrew M. and MacInnis, Martin J. and Baillie, J. Kenneth and {The Lake Louise AMS Score Consensus Committee} and Achatz, Eric and Albert, Edi and Andrews, Jon S. and Anholm, James D. and Ashraf, Mohammad Zahid and Auerbach, Paul and Basnyat, Buddha and Beidleman, Beth A. and Berendsen, R.R. and Berger, Marc Moritz and Bloch, Konrad E. and Brugger, Hermann and Cogo, Annalisa and Costa, Ricardo Gonzalez and Cumpstey, Andrew and Cymerman, Allen and Debevec, Tadej and Duncan, Catriona and Dubowitz, David and Fago, Angela and Furian, Michael and Gaidica, Matt and Ganguli, Prosenjit and Grocott, Michael P.W. and Hammer, Debra and Hall, David and Hillebrandt, David and Hilty, Matthias Peter and Himashree, Gigugu and Honigman, Benjamin and {Gilbert-Kawai}, Ned and Kayser, Bengt and Keyes, Linda and Koehle, Michael and Kohli, Samantha and Kuenzel, Arlena and Levine, Benjamin D. and Lichtblau, Mona and Macdonald, Jamie and Maeder, Monika Brodmann and Maggiorini, Marco and Martin, Daniel and Masuyama, Shigeru and McCall, John and McIntosh, Scott and Millet, Gregoire and Moraga, Fernando and Mounsey, Craig and Muza, Stephen R. and Oliver, Samuel and Pasha, Qadar and Paterson, Ryan and Phillips, Lara and Pichon, Aur{\'e}lien and Pickerodt, Philipp A. and Pun, Matiram and Rain, Manjari and Rennie, Drummond and {Ri-Li}, Ge and Roy, Steven and Verges, Samuel and {dos Santos}, Tatiana Batalha Cunha and Schoene, Robert B. and Schoch, Otto D. and Singh, Surinderpal and Sooronbaev, Talant and Steinback, Craig D. and Stembridge, Mike and Stewart, Glenn and Stobdan, Tsering and Strapazzon, Giacomo and Subudhi, Andrew W. and Swenson, Erik and Roger Thompson, A. A. and {van Patot}, Martha Tissot and Twomey, Rosie and Ulrich, Silvia and Voituron, Nicolas and Wagner, Dale R. and Wang, Shih-hao and West, John B. and Wilkes, Matt and Willmann, Gabriel and Yaron, Michael and Zafren, Ken},
